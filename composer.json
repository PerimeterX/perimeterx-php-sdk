{
  "name": "perimeterx/php-sdk",
  "description": "PerimeterX SDK for PHP",
  "version" : "2.0.0",
  "keywords": [
    "perimeterx",
    "websecurity",
    "security",
    "bots",
    "sdk"
  ],
  "license": "PerimeterX Inc",
  "hompage": "http://www.perimeterx.com",
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "php": ">=5.5",
    "psr/log": "^1.0.2"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Perimeterx\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Perimeterx\\Tests\\": "tests/"
    }
  }
}
